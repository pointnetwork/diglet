  "bugs": {
    "url": "https://github.com/pointnetwork/diglet/issues"
  },
  "homepage": "https://github.com/pointnetwork/diglet#readme",
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^5.16.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "pem": "^1.14.2",
    "pkg": "^4.3.3",
    "ws": "^5.2.2"
  },
  "dependencies": {
    "async": "^2.6.1",
    "bunyan": "^1.8.5",
    "commander": "^2.9.0",
    "merge": "^1.2.1",
